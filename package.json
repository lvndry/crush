{
  "name": "jazz-cli",
  "version": "0.1.0",
  "description": "A powerful agentic automation CLI for managing agentic loops in daily life workflows",
  "main": "dist/main.js",
  "bin": {
    "jazz": "./dist/main.js"
  },
  "scripts": {
    "build": "bun run tsc",
    "dev": "bun --watch src/main.ts",
    "start": "bun dist/main.js",
    "cli": "bun src/main.ts",
    "lint": "bunx eslint src/**/*.ts",
    "lint:fix": "bunx eslint src/**/*.ts --fix",
    "format": "bunx prettier --write src/**/*.ts",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:type-check": "bunx tsc --project tsconfig.test.json",
    "clean": "rm -rf dist",
    "prebuild": "bun run clean",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "cli",
    "agents",
    "automation",
    "agentic",
    "typescript",
    "effect-ts"
  ],
  "author": {
    "name": "lvndry",
    "email": "lvndry@proton.me",
    "url": "https://github.com/lvndry"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lvndry/jazz.git"
  },
  "homepage": "https://github.com/lvndry/jazz#readme",
  "bugs": {
    "url": "https://github.com/lvndry/jazz/issues"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.17",
    "@ai-sdk/google": "^2.0.14",
    "@ai-sdk/mistral": "^2.0.14",
    "@ai-sdk/openai": "^2.0.32",
    "@ai-sdk/xai": "^2.0.20",
    "@effect/platform": "^0.90.9",
    "@effect/platform-node": "^0.96.1",
    "@effect/schema": "^0.75.5",
    "ai": "^5.0.47",
    "chalk": "^5.6.2",
    "commander": "^14.0.1",
    "dotenv": "^17.2.2",
    "effect": "^3.17.13",
    "googleapis": "^159.0.0",
    "inquirer": "^12.9.4",
    "linkup-sdk": "^2.3.0",
    "marked": "^16.3.0",
    "marked-terminal": "^7.3.0",
    "open": "^10.2.0",
    "short-uuid": "^5.0.1",
    "zod": "^4.1.9"
  },
  "devDependencies": {
    "@types/bun": "^1.2.22",
    "@types/inquirer": "^9.0.9",
    "@types/marked": "^6.0.0",
    "@types/marked-terminal": "^6.1.1",
    "@types/node": "^24.3.3",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ]
}
